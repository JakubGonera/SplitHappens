<AnchorPane prefHeight="200.0" prefWidth="300.0" styleClass="background-color-1" stylesheets="@Style/panes.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <Label stylesheets="@Style/interactables.css" text="Are you sure you want to delete this person?" />
            <Button mnemonicParsing="false" onAction="#onClickDelete" stylesheets="@Style/interactables.css" text="Delete">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
